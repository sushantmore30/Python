{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sushant-adf-18"
		},
		"PostgreSqlLs_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PostgreSqlLs'"
		},
		"storagels_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storagels'"
		},
		"PostgreSqlLs_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"PostgreSqlLs_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"PostgreSqlLs_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"storagels_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sushantdl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sushant-adf-18_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storagels_accountKey": {
						"value": "[parameters('storagels_accountKey')]"
					},
					"PostgreSqlLs_password": {
						"value": "[parameters('PostgreSqlLs_password')]"
					},
					"storagels_properties_typeProperties_url": {
						"value": "[parameters('storagels_properties_typeProperties_url')]"
					},
					"PostgreSqlLs_properties_typeProperties_server": {
						"value": "[parameters('PostgreSqlLs_properties_typeProperties_server')]"
					},
					"PostgreSqlLs_properties_typeProperties_database": {
						"value": "[parameters('PostgreSqlLs_properties_typeProperties_database')]"
					},
					"PostgreSqlLs_properties_typeProperties_username": {
						"value": "[parameters('PostgreSqlLs_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}