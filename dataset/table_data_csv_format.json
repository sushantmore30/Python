{
	"name": "table_data_csv_format",
	"properties": {
		"linkedServiceName": {
			"referenceName": "storagels",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schemaname": {
				"type": "string"
			},
			"tablename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{concat(dataset().tablename),'.csv'}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@{concat(dataset().schemaname,'/',dataset().tablename)}",
					"type": "Expression"
				},
				"fileSystem": "bronze"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": false,
			"quoteChar": "\""
		},
		"schema": []
	}
}